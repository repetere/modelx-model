{
  "name": "@jsonstack/model",
  "version": "1.1.1",
  "description": "Deep Learning Classification, LSTM Time Series, Regression and Multi-Layered Perceptrons with Tensorflow",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "browser": "dist/web/index.js",
  "web": "dist/web/web.js",
  "modules.root": "dist/esm",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/repetere/jsonm-model.git"
  },
  "keywords": [
    "regression",
    "classification",
    "supervised",
    "learning",
    "supervised learning",
    "dl",
    "deep learning",
    "tensorflow",
    "machine learning",
    "ai"
  ],
  "author": {
    "name": "Yaw Etse",
    "email": "yaw.etse@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/repetere/jsonm-model/issues"
  },
  "homepage": "https://github.com/repetere/jsonm-model#readme",
  "scripts": {
    "dev": "tsc -c -w",
    "benchmark": "node -r esm  benchmark/main_bmrk.js",
    "test": "jest --coverage",
    "doc": "tsc -p tsconfig.esm.json && typedoc && sitedown manual/ -b docs/manual",
    "compile": "npm run compile:cjs && npm run compile:esm && rollup -c",
    "compile:cjs": "tsc -p tsconfig.cjs.json ",
    "compile:esm": "tsc -p tsconfig.esm.json ",
    "build": "tsc -p tsconfig.esm.json && typedoc && sitedown manual/ -b docs/manual",
    "coverage": "coveralls"
  },
  "dependencies": {
    "@tensorflow-models/universal-sentence-encoder": "^1.3.2",
    "@tensorflow/tfjs": "^3.3.0",
    "@tensorflow/tfjs-node": "^3.3.0",
    "axios": "^0.21.1",
    "lodash.range": "3.2.0",
    "tsne-js": "1.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@jsonstack/data": "1.1.3",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-sucrase": "^3.1.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/npm": "^7.1.0",
    "@semantic-release/release-notes-generator": "9.0.3",
    "@tensorflow/tfjs-converter": "^2.8.6",
    "@tensorflow/tfjs-core": "^2.8.6",
    "@types/fs-extra": "^9.0.9",
    "@types/jest": "^26.0.22",
    "@types/lodash.range": "3.2.6",
    "fs-extra": "^10.0.0",
    "highcharts": "^9.0.1",
    "highcharts-export-server": "^2.1.0",
    "jest": "^27.0.6",
    "rollup": "^2.44.0",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-terser-js": "1.0.2",
    "ts-jest": "^26.5.4",
    "tslib": "^2.1.0",
    "typedoc": "^0.21.2",
    "typescript": "^4.2.3"
  },
  "jest": {
    "testEnvironment": "node",
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "extensionsToTreatAsEsm": [
          ".ts",
          ".js"
        ],
        "useESM": true,
        "diagnostics": false,
        "tsconfig": {
          "target": "ES2019",
          "allowJs": true
        }
      }
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "package-lock.json",
          "CHANGELOG.md"
        ],
        "message": "Release: ${nextRelease.version}\n\n${nextRelease.notes}"
      }
    ],
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "docs",
              "scope": "README",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "style",
              "release": "patch"
            }
          ],
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES"
            ]
          }
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/changelog"
    ]
  }
}
